@model LeeInfo.Data.Forex.FrxPosition
@{ 
    Layout = null;
}
<section class="content-header">
    <h1>
        Details
        <small>FrxPosition</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Forex</a></li>
        <li><a asp-action="Index"> FrxPosition</a></li>
        <li class="active"> Details</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <!-- left column -->
        <div class="col-md-12">
            <!-- general form elements -->
            <div class="box box-primary">
                <!-- form start -->
                    @await Html.PartialAsync("_ModalHeader", new ModalHeader { Heading = "Details\tFrxPosition" })
                    @*<div class="box-header with-border">
                        </div>*@
                    <!-- /.box-header -->
                <div class="box-body">
                    <div class="form-group">
                        <label asp-for="SymbolCode" class="control-label"></label>
                        <input asp-for="SymbolCode" class="form-control" placeholder="@Model.SymbolCode" disabled="disabled" />
                    </div>
                    <div class="form-group">
                        <label asp-for="TradeType" class="control-label"></label>
                        <input asp-for="TradeType" class="form-control" placeholder="@Model.TradeType" disabled="disabled" />
                    </div>
                    <div class="form-group">
                        <label asp-for="Quantity" class="control-label"></label>
                        <input asp-for="Quantity" class="form-control" placeholder="@Model.Quantity" disabled="disabled" />
                    </div>
                    <div class="form-group">
                        <label asp-for="Label" class="control-label"></label>
                        <input asp-for="Label" class="form-control" placeholder="@Model.Label" disabled="disabled" />
                    </div>
                    <div class="form-group">
                        <label asp-for="Comment" class="control-label"></label>
                        <input asp-for="Comment" class="form-control" placeholder="@Model.Comment" disabled="disabled" />
                    </div>
                    <div class="form-group">
                        <label asp-for="EntryPrice" class="control-label"></label>
                        <input asp-for="EntryPrice" class="form-control" placeholder="@Model.EntryPrice" disabled="disabled" />
                    </div>
                    <div class="form-group">
                        <label asp-for="EntryTime" class="control-label"></label>
                        <input asp-for="EntryTime" class="form-control" placeholder="@Model.EntryTime" disabled="disabled" />
                    </div>
                </div>
                    <!-- /.box-body -->
                    @*<div class="box-footer col-md-offset-8">
                                <button type="submit">提交</button><button asp-action="Index">返回</button>
                        </div>*@
            </div>
            <!-- /.box -->
        </div>
    </div>
</section>
<!-- /.content -->

